Suppose N=15770708441. B=180 then p=11620221425 and d=135979

B = 10^4

N = 357177030895805008781319266876641103581839678151495135129133067010127
#N = 5000272002251811540446579586816039346308785565641862331905860763123733 
#N = 111293094034769304884167051458174320813595510448138274866152002067365927 
#N = 189114989429752082926285457551369642787381790039260802307452110490304547 
#N = 250733654482468568921620042866859718852920028026076547177974758239109177 
#N = 201557389900540095613559219541299540522405259329399736824858252876376521311053006710577163057234093
#N = 862021547643631582396998212208722914288258644234791307950582916442747222039795609417741932278317121 

c = power_mod(2, factorial(B), N)
#c = mod((pow(2, factorial(B))), N)

p = gcd(c-1, N)

q = N/p

print "c =", c
print "N =", N
print "p =", p
print "q =", q
print "CHECK p*q ", p*q